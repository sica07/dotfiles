! font settings --------------------------------------------------------------

Xft.autohint: true
Xft.antialias: true
Xft.hinting: full
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #222222, #222222, #1e2529
rofi.color-window: #222222, #222222, #b1b4b3
rofi.color-normal: #222222, #c1c1c1, #222222, #394249, #ffffff
rofi.color-active: #222222, #80cbc4, #222222, #394249, #80cbc4
rofi.color-urgent: #222222, #ff1844, #222222, #394249, #ff1844
rofi.font: Iosevka Term Light 13
!------------------------------------------------------------------------------
! URXVT - Settings

!URxvt.depth: 32
URxvt.scrollBar: false
URxvt.perl-ext-common: default,clipboard,matcher
! URxvt.perl-ext-common: default,clipboard,matcher,tabbed
URxvt.url-launcher:		/usr/bin/firefox
URxvt.matcher.button:   1
URxvt*font:     xft:Iosevka Term Light:size=13
!URxvt*boldFont: xft:Iosevka Term Bold

!URxvt*font:     xft:FuraCode Nerd Font Mono:regular:size=13
!URxvt*boldFont: xft:Menlo for Powerline Bold:bold:size=14
URxvt.termName: rxvt-256color
!URxvt.lineSpace: 8

urxvt*background:       #0c0d0e
urxvt*foreground:       #7f8f9f
urxvt*scrollBar:        false
urxvt*matcher.button:   1
urxvt*allow_bold:    true


!URxvt*geometry: 85x16
