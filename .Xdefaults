! font settings --------------------------------------------------------------

Xft.autohint: true
Xft.antialias: true
Xft.hinting: full
Xft.hintstyle: hintslight
Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #273238, #273238, #1e2529
rofi.color-normal: #273238, #c1c1c1, #273238, #394249, #ffffff
rofi.color-active: #273238, #80cbc4, #273238, #394249, #80cbc4
rofi.color-urgent: #273238, #ff1844, #273238, #394249, #ff1844
!------------------------------------------------------------------------------
! URXVT - Settings

!URxvt.depth: 32
URxvt.scrollBar: false
URxvt.perl-ext-common: default,clipboard,matcher
! URxvt.perl-ext-common: default,clipboard,matcher,tabbed
URxvt.url-launcher:		/usr/bin/firefox
URxvt.matcher.button:   1
URxvt*font:     xft:FuraCode Nerd Font Mono:regular:size=12
!URxvt*boldFont: xft:Menlo for Powerline Bold:bold:size=14
URxvt.termName: rxvt-256color
!URxvt.lineSpace: 8

!urxvt*background:       #0c0d0e
!urxvt*foreground:       #7f8f9f
urxvt*scrollBar:        false
urxvt*matcher.button:   1
urxvt*allow_bold:    true


!URxvt*geometry: 85x16
URxvt*fading:        0
URxvt*tintColor:     #ffffff
URxvt*shading:        0
URxvt*inheritPixmap:     False

