    bash ~/dotfiles/scripts/pass.sh

super+shift+g
    flashpeak-slimjet
super+shift+f
    firefox
super+e
    pcmanfm
super+c
    tty-clock -cf -C 4

XF86AudioLowerVolume
    amixer set Master 10%-
XF86AudioRaiseVolume
    amixer set Master 10%+
XF86AudioMute
    amixer set Master toggle
XF86AudioPlay
    playerctl play-pause
XF86AudioPause
    playerctl pause
XF86AudioNext
    playerctl next
XF86AudioPrev
    playerctl previous


